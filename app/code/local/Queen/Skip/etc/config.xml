<?xml version="1.0"?>
<config>
    <modules>
        <Queen_Skip>
            <version>0.1.0</version>
        </Queen_Skip>
    </modules>
    <global>
        <models>
            <queen_skip>
                <class>Queen_Skip_Model</class>
                <resourceModel>queen_skip_resource</resourceModel>
            </queen_skip>
            <queen_skip_resource>
                <class>Queen_Skip_Model_Resource</class>
            </queen_skip_resource>
        </models>
        <blocks>
            <queen_skip>
                <class>Queen_Skip_Block</class>
            </queen_skip>
        </blocks>
        <helpers>
            <queen_skip>
                <class>Queen_Skip_Helper</class>
            </queen_skip>
        </helpers>
        <resources>
            <queen_skip_setup>
                <setup>
                    <module>Queen_Skip</module>
                </setup>
            </queen_skip_setup>
        </resources>
    </global>
    <adminhtml>
        <layout>
            <updates>
                <skip>
                    <file>skip.xml</file>
                </skip>
            </updates>
        </layout>
    </adminhtml>
    <stores>
        <admin>
            <design>
                <theme>
                    <template>skip</template>
                </theme>
            </design>
        </admin>
    </stores>
</config>